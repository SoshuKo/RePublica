    },
    {
      "type": "line",
      "self": {
        "name": "アカウ",
        "file": "アカウ_悲.png",
        "visible": true
      },
      "enemy": {
        "name": "タネイ",
        "file": "タネイ.png",
        "visible": true
      },
      "speech": {
        "side": "self",
        "name": "アカウ",
        "text": "「話があります」\n「実はわたしは」"
      }
    },
    {
      "type": "line",
      "self": {
        "name": "アカウ",
        "file": "アカウ_悲.png",
        "visible": true
      },
      "enemy": {
        "name": "タネイ",
        "file": "タネイ.png",
        "visible": true
      },
      "speech": {
        "side": "self",
        "name": "ナレーション",
